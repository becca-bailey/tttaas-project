echo The Setup Process will begin now! 
git clone https://github.com/beccanelson/tttaas-clojure.git ./lib/tttaas-clojure/ 
git clone https://github.com/beccanelson/tttaas-server.git ./lib/tttaas-server/
cd ./lib/tttaas-clojure
git pull
lein uberjar
cd ../tttaas-server/
git pull
mvn package
cd ../.. 
mvn clean 
mvn install:install-file -Dfile=./lib/tttaas-clojure/target/tic-tac-toe-0.1.0-SNAPSHOT-standalone.jar -DgroupId=tic-tac-toe -DartifactId=tic-tac-toe -Dversion=0.1.0-SNAPSHOT -Dpackaging=jar 
mvn install:install-file -Dfile=./lib/tttaas-server/target/server-0.0.1.jar -DgroupId=becca-tom -DartifactId=server -Dversion=0.0.1 -Dpackaging=jar
mvn package 
